{
  "manifest_version": 2,
  "name": "LoL Stream Opener",
  "version": "1.1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "permissions": [
    "tabs",
    "storage",
    "https://leaguedrops.onrender.com/*",
    "https://lolesports.com/*"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
